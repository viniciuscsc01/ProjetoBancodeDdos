@model ProjetoBancoDeDados.Models.Inscricoes

@{
    ViewData["Title"] = "Imprimir";
}

<h1>Imprimir</h1>

<div>
    <h4>Inscricoes</h4>
    <hr />
    <dl class="row">
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.Eventos)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Eventos.Nome)
        </dd>
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.PagamentoId)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.PagamentoId.ValorId)
        </dd>
