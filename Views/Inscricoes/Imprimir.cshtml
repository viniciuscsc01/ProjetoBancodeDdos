            @Html.DisplayNameFor(model => model.Usuarios)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Usuarios.NomeId)
        </dd>
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.Passagens)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Passagens.DescricaoId)
        </dd>
