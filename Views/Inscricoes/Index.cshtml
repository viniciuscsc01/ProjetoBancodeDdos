@model IEnumerable<ProjetoBancoDeDados.Models.Inscricoes>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Eventos)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.PagamentoId)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Usuarios)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Passagens)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Eventos.Id)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.PagamentoId.Id)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Usuarios.Id)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Passagens.Id)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.Id">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.Id">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.Id">Delete</a>
                    <a asp-action="Imprimir" asp-route-id="@item.Id">Imprimir</a> |
            </td>
        </tr>
}
    </tbody>
</table>
